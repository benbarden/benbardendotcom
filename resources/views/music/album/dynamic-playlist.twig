<script type="text/javascript">

    var bbMusicTracklist = [];

    {% for item in PlaylistItems %}
bbMusicTracklist.push({
        title: "{{ item.track.title }}",
        mp3: encodeURI('{{ item.track.mediaFolder.path }}{{ item.track.media_file }}'),
        duration: "{{ item.track.duration }}"
        {% if item.track.info_url %}
        , infoUrl: "{{ item.track.info_url }}"
        {% endif %}
        {% if item.track.year_released %}
        , yearReleased: "{{ item.track.year_released }}"
        {% endif %}
    });
    {% endfor %}

</script>